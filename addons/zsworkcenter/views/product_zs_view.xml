<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Inherit Form view -->
    <record id="product_template_form_view_inherit_product_zs" model="ir.ui.view">
        <field name="name">product.template.common.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <page name="general_information" position="after">
                <page string="至尚定义">
                    <group col="4">
                        <group colspan="2" col="2">
                            <field name="plat_code"  placeholder="交易平台编码" />
                            <field name="is_puyao"  string="普药/新药" />
                            <field name="model" />
                            <field name="the_form_of_a_drag" placeholder="剂型"/>
                            <field name="lis_number"/>
                            <field name="buyer" />
                            <field name="zstype"/>
                            <field name="product_class"/>
                            <field name="manufacturing_enterprise"/>
                            <field name="bid_enterprise" />
                        </group>
                        <group  colspan="2" col="2">
                            <field name="packing" />
                            <field name="package_count"/>
                            <field name="dk_price" />
                            <field name="floor_price" />
                            <field name="cesd" />
                            <field name="txsj" />
                            <field name="fkamount" />
                            <field name="fk_cycle" />
                            <field name="fkfangshi" string="付款方式" />
                            <field name="xsht" />
                        </group>
                    </group>
                </page>
            </page>
        </field>
    </record>
</odoo>